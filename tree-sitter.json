{
  "$schema": "https://tree-sitter.github.io/tree-sitter/assets/schemas/config.schema.json",
  "grammars": [
    {
      "name": "robots_txt",
      "camelcase": "RobotsTxt",
      "title": "robots_txt",
      "scope": "source.robots",
      "file-types": [
        "robots"
      ],
      "injection-regex": "^robots$",
      "class-name": "TreeSitterRobotsTxt"
    }
  ],
  "metadata": {
    "version": "1.0.1",
    "license": "MIT",
    "description": "Grammar for robots.txt",
    "authors": [
      {
        "name": "Vladimir <opa_oz> Levin",
        "email": "opaozhub@protonmail.com",
        "url": "https://github.com/opa-oz"
      }
    ],
    "links": {
      "repository": "https://github.com/opa-oz/tree-sitter-robots-txt",
      "funding": "https://ko-fi.com/opa_oz"
    }
  },
  "bindings": {
    "c": true,
    "go": true,
    "node": true,
    "python": true,
    "rust": true,
    "swift": true,
    "zig": false
  }
}
